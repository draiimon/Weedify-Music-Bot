# Discord Bot Token
TOKEN=your_discord_bot_token_here

# MongoDB Database
MONGODB_URI=your_mongodb_uri_here

# Genius Lyrics API
GENIUS_API_KEY=your_genius_api_key_here

# Groq Whisper API for Voice Recognition
GROQ_API_KEY=your_groq_api_key_here

# Bot Configuration
BOT_PREFIX=w!

# Lavalink Configuration (Optional - uses defaults if not set)
LAVALINK_HOST=lava-all.ajieblogs.eu.org
LAVALINK_PORT=443
LAVALINK_PASSWORD=https://dsc.gg/ajidevserver
LAVALINK_SECURE=true

# Voice Recognition Settings
SPEECH_METHOD=groq
VOICE_LANGUAGE=en
VOICE_AUTO_ENABLE=false

# Uptime Management for Render 24/7
# IMPORTANT: Set RENDER_EXTERNAL_URL to your actual Render app URL
# Example: https://weedify-bot.onrender.com
SELF_PING_ENABLED=true
SELF_PING_INTERVAL=840000
RENDER_EXTERNAL_URL=https://your-app-name.onrender.com
